create procedure [dbo].[A_CATEGORIA]
(@nom varchar(50))
as
insert into CATEGORIA values(@nom,'A')
GO
/****** Object:  StoredProcedure [dbo].[A_PRODUCTO]    Script Date: 30/04/2019 13:49:22 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create procedure [dbo].[A_PRODUCTO](
@id char(8),
@nom varchar(180),
@id_cat int,
@pre float,
@stock int,
@img varchar(50)
)
as
INSERT INTO producto VALUES (@id,@nom,@id_cat,@pre,@stock,@img,'AD0001',GETDATE(),'A')
GO
/****** Object:  StoredProcedure [dbo].[A_USUARIO]    Script Date: 30/04/2019 13:49:22 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create procedure [dbo].[A_USUARIO](
@cod char(8),
@nom varchar(50),
@ape varchar(50),
@correo varchar(50),
@Telefono varchar(50),
@nomUsu varchar(50),
@contra varchar(50)
)
as
insert into usuario values (@cod,@nom,@ape,'Cliente',@correo,@Telefono,GETDATE(),'A',@contra,@nomUsu);
GO
/****** Object:  StoredProcedure [dbo].[B_ProdxNom]    Script Date: 30/04/2019 13:49:22 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create procedure [dbo].[B_ProdxNom](
@nom varchar(50)
)
as
select*from PRODUCTO where estado='A' and NOM_PRO like (@nom+'%')
GO
/****** Object:  StoredProcedure [dbo].[B_ProdxNomADMIN]    Script Date: 30/04/2019 13:49:22 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
create procedure [dbo].[B_ProdxNomADMIN](
@nom varchar(50)
)
as
select*from PRODUCTO where NOM_PRO like (@nom+'%')
GO
